					"script": {
						"exec": [
							"pm.test(\"Response time is less than 2 Sec\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(2000);\r",
							"});\r",
							"pm.test(\"Status code is 2xx\", function () {\r",
							"    pm.response.to.have.status(200, 201, 202);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Check The Data Responded\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.page).to.eql(1);\r",
							"    pm.expect(jsonData.total).to.eql(12);\r",
							"    pm.expect(jsonData.total_pages).to.eql(2);\r",
							"    pm.expect(jsonData.data[0].email).to.eql(\"george.bluth@reqres.in\");\r",
							"    \r",
							"});"
